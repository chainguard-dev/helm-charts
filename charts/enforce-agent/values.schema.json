{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "auth": {
            "type": "object",
            "properties": {
                "gcp": {
                    "type": "object",
                    "properties": {
                        "serviceAccount": {
                            "type": "object",
                            "properties": {
                                "email": {
                                    "type": "string"
                                },
                                "key": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "clusterName": {
            "type": "string"
        },
        "enforcerOptions": {
            "type": "object",
            "properties": {
                "enableCIPCache": {
                    "type": "boolean"
                },
                "namespaceEnforcementMode": {
                    "type": "string"
                },
                "webhookFailOpen": {
                    "type": "boolean"
                },
                "noMatchPolicy": {
                    "type": "string"
                }
            }
        },
        "inviteCode": {
            "type": "string"
        }
    }
}
